<p id="notice"><%= notice %></p>

<h1>Phones Nearby You</h1>

<table>
  <thead>
    <tr>
      <th>Tel</th>
      <%# <th>La</th>
      <th>Lo</th>
      <th>Ip</th>
      <th colspan="3"></th> %>
    </tr>
  </thead>

  <tbody>
    <% @calls.each do |call| %>
      <tr>
        <% if session[:current_tel] != call.tel %>
          <td><a href="tel:<%= call.tel %>"><%= call.tel %></a></td>
        <% end %>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'End', Call.find(session[:current_id]), method: :delete, data: { confirm: 'Stop to make your call visible for people nearby you?' } %>

